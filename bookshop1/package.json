{
	"name": "bookshop1",
	"version": "0.0.1",
	"private": true,
	"sapux": true,
	"description": "A Fiori application.",
	"keywords": [
		"ui5",
		"openui5",
		"sapui5"
	],
	"main": "webapp/index.html",
	"scripts": {
		"start": "fiori run --open 'test/flpSandbox.html#bookshop1-tile'",
		"start-local": "fiori run --config ./ui5-local.yaml --open 'test/flpSandbox.html#bookshop1-tile'",
		"start-noflp": "fiori run --open 'index.html'",
		"build": "ui5 build -a --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",
		"deploy": "fiori verify",
		"deploy-config": "fiori add deploy-config",
		"int-tests": "fiori run --config ./ui5-mock.yaml --open 'test/integration/opaTests.qunit.html'",
		"start-mock": "fiori run --config ./ui5-mock.yaml --open 'test/flpSandbox.html#bookshop1-tile'"
	},
	"devDependencies": {
		"@ui5/cli": "^2.10.4",
		"@sap/open.fe": "1.85.0",
		"@ui5/fs": "^2.0.6",
		"@ui5/logger": "^2.0.1",
		"@sap/ux-ui5-tooling": "1",
		"rimraf": "3.0.2",
		"@sap/ux-specification": "latest",
		"@sap/ux-ui5-fe-mockserver-middleware": "1.1.4"
	},
	"ui5": {
		"dependencies": [
			"@sap/ux-ui5-tooling",
			"@sap/open.fe",
			"@sap/ux-ui5-fe-mockserver-middleware"
		]
	}
}
