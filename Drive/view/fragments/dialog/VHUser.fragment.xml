<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Users"
		class="sapUiPopupWithPadding"
		items="{path: '/VHUsers'}"
		search="_handleValueHelpSearch"
		confirm="VHUsersDialogClose"
		cancel="VHUsersDialogClose"
		growing="false"
		multiSelect="true">
		<StandardListItem
			title="{DefaultFullName}"
			description="{Id}" />
	</SelectDialog>
</core:FragmentDefinition>